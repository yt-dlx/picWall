{
  "expo": {
    "owner": "shovit",
    "slug": "picbook",
    "version": "2.0.0",
    "name": "picbook",
    "scheme": "picbook",
    "orientation": "portrait",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shovit.picbook"
    },
    "userInterfaceStyle": "automatic",
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./assets/picbook/picbook_white_nobg.png",
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./assets/picbook/picbook_black_bg.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bf8ce3fd-d2bc-4897-ab27-b6b34c9a428f"
      }
    },
    "android": {
      "package": "com.shovit.picbook",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/picbook/picbook_white_nobg.png"
      }
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Kurale.ttf",
            "./assets/fonts/Brittany.otf"
          ]
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000",
          "image": "./assets/picbook/picbook_white_nobg.png"
        }
      ]
    ]
  }
}
