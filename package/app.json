{
  "expo": {
    "owner": "shovit",
    "slug": "picbook",
    "version": "4.0.0",
    "name": "picbook",
    "scheme": "picbook",
    "orientation": "portrait",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "experiments": { "typedRoutes": true },
    "icon": "./assets/picbook/splash/picbook_white_nobg.png",
    "ios": { "supportsTablet": true, "bundleIdentifier": "com.shovit.picbook" },
    "web": { "output": "static", "bundler": "metro", "favicon": "./assets/picbook/splash/picbook_black_bg.png" },
    "extra": { "router": { "origin": false }, "eas": { "projectId": "bf8ce3fd-d2bc-4897-ab27-b6b34c9a428f" } },
    "android": { "package": "com.shovit.picbook", "adaptiveIcon": { "backgroundColor": "#000000", "foregroundImage": "./assets/picbook/splash/picbook_white_nobg.png" } },
    "plugins": [
      ["expo-font", { "fonts": ["./assets/fonts/Kurale.ttf", "./assets/fonts/Brittany.otf"] }],
      "expo-router",
      ["expo-splash-screen", { "imageWidth": 200, "resizeMode": "contain", "backgroundColor": "#000000", "image": "./assets/picbook/splash/picbook_white_nobg/splash_3200x3200.png" }]
    ]
  }
}
