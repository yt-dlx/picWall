{
  "expo": {
    "version": "1.0.0",
    "owner": "shovit",
    "slug": "pikWall",
    "name": "pikWall",
    "scheme": "pikWall",
    "orientation": "portrait",
    "newArchEnabled": true,
    "userInterfaceStyle": "automatic",
    "experiments": {
      "typedRoutes": true
    },
    "icon": "./src/assets/pikWall/pikWall_red.png",
    "ios": {
      "supportsTablet": true,
      "requiresFullScreen": true,
      "bundleIdentifier": "com.shovit.pikWall",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "pikWall: needs permission to save photos to your library.",
        "NSPhotoLibraryUsageDescription": "pikWall: requires access to your photo library to save images."
      }
    },
    "web": {
      "output": "static",
      "bundler": "metro",
      "favicon": "./src/assets/pikWall/pikWall_red.png"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8d28e42e-f194-40ac-b49e-442b27d0d6ef"
      }
    },
    "android": {
      "requiresFullScreen": true,
      "package": "com.shovit.pikWall",
      "permissions": ["READ_EXTERNAL_STORAGE", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A",
        "foregroundImage": "./src/assets/pikWall/pikWall_red.png"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./src/assets/fonts/Kurale.ttf", "./src/assets/fonts/Brittany.otf"]
        }
      ],
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0A0A0A",
          "image": "./src/assets/pikWall/pikWall_red.png"
        }
      ]
    ]
  }
}
